        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd"
>

    <bean id="consoleWriter1" class="com.babor.spring.ConsoleWriter">
    </bean>

    <bean id="consoleWriter2" class="com.babor.spring.ConsoleWriter">
